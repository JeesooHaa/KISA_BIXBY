layout-macro-def(travelDetailInfoOut) {
  params {
    param (travelset) {
      type (travelSet)
      min (Optional)
      max (One)
    }
  }

  content {
    section {  // this macro returns a "section"
      content {
        paragraph {
          value ("#{value(travelset.kind)}")
          style (Detail_M)
        }
      }
    }
  }
}
