layout-macro-def(insureDetailInfoOut) {
  params {
    param (insureset) {
      type (insureSet)
      min (Optional)
      max (One)
    }
  }

  content {
    section {  // this macro returns a "section"
      content {
        paragraph {
          value ("#{value(insureset.rate)}")
          style (Detail_M)
        }
      }
    }
  }
}
